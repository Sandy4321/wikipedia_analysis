!style="color:#ffffff; background:#ff0000;" scope="col" | Lieu
!style="color:#ffffff; background:#ff0000;" scope="col" | Match
!style="color:#ffffff; background:#ff0000;" scope="col" | Score
!style="color:#ffffff; background:#ff0000;" scope="col" | Compétition
