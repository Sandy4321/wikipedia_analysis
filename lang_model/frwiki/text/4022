 | teamB       = [[Élan sportif chalonnais|Chalon-sur-Saône]]
 | scoreB      = 82
 | overtime    =
 | Q1          = '''30'''-16
 | Q2          = 23-'''28'''
 | Q3          = 20-'''21'''
 | Q4          = '''20'''-17
 | OT          = 
 | H1          = '''53'''-44
 | H2          = '''40'''-38
 | points1     = [[Antoine Diot]] 18
 | rebounds1   = [[David Andersen]] 6
 | assist1     = [[Louis Campbell]] 8
