 | attendance  = {{formatnum:6170}}<ref>{{Lien web|url=http://sigbasket.fr/2014/06/cruelle-desillusion/|titre=Cruelle désillusion !|site=sigbasket.fr|auteur=|date=2 juin 2014}}</ref>
