{{basketballbox
 | bg         = #eeeeee
 | date       = {{date sport|5|août|2012|en basket-ball}}
 | heure      = 11 h 15 [[British Summer Time|BST]]
 | report     = [http://www.london2012.com/basketball/event/women/match=bkw400a13/index.html Rapport]
 | teamA      = {{Angola basket-ball}}
 | scoreA     = 47
