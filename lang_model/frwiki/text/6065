 | scoreA     = '''76'''
 | teamB      = {{TUR-d}} Turquie
 | scoreB     = 23
 | overtime   =
 | Q1         = '''17'''-8
 | Q2         = '''23'''-4
 | Q3         = '''22'''-5
 | Q4         = '''14'''-6
 | OT         = 
 | H1         = '''40'''-12
 | H2         = '''36'''-11
 | points1    = Hamer 20
 | rebounds1  = Hamer 9
 | assist1    = Freeman 10
 | evaluation1= Freeman 25
 | points2    = Çakir 15
 | rebounds2  = Çakir 11
 | assist2    = Emuce 1
 | evaluation2= Unal 5
 | place      = [[Université de Worcester|Worcester University Arena]], Court central<br/>{{GBR-d}} [[Worcester]]
 | attendance = 
