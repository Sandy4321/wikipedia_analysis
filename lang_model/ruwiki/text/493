 |рейт1           = {{Rating|4.5|5}}<ref name="allmusic">{{cite web|author=Steve Huey|title=Quality - Talib Kweli|url=http://www.allmusic.com/album/quality-mw0000662603|publisher=[[AllMusic]]|accessdate=2016-02-12|language=en}}</ref>
 
 |ист2            = ''[[Alternative Press]]''
