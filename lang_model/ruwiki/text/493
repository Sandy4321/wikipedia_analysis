 |ист3            = ''{{нп3|The A.V. Club}}''
 |рейт3           = A<ref name="avclub">{{cite web|author=Nathan Rabin|title=Talib Kweli: Quality|url=http://www.avclub.com/review/talib-kweli-emqualityem-12174|publisher={{нп3|The A.V. Club}}|accessdate=2016-02-12|language=en}}</ref>
 
