|дата = [[20 декабря]] [[1969]]
|команда1 = Пеньяроль {{Флаг Уругвая}}
|команда2 = {{Флаг Аргентины}} Расинг
|счёт = 4:1
|стадион = [[Сентенарио (стадион)|Сентенарио]] — [[Монтевидео]]
|голы1 = [[Спенсер, Альберто|Спенсер]] 14', 44'<br />[[Роча, Педро|Роча]] 55' (пен.), 72'
|голы2 = [[Перфумо, Роберто|Перфумо]] 89'
|зрители = 22 000
