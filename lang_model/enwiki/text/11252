{{Tracklist
| collapsed       = yes
| writing_credits = no
| headline        = Deluxe Edition DVD Contents<ref name="sonybmg1">{{cite web|url=http://www.sonybmg.de/artists2.php?iA=7&artist=176644&product=88697407762 |title=Britney Spears - Releases - Sony Music Entertainment Germany GmbH |publisher=Sonybmg.de |date= |accessdate=2009-03-22}}</ref>
