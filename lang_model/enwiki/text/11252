| rev6 = ''[[The Independent]]''
| rev6Score = {{Rating|3|5}}<ref>{{cite news|url=http://www.independent.co.uk/arts-entertainment/music/reviews/album-britney-spears-circus-zombasony-1038411.html |title=The Independent review | location=London | date=2008-11-28 | accessdate=2010-04-30}}</ref>
| rev7 = ''[[NME]]''
